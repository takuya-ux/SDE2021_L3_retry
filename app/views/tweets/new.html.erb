
<div id="wrapper">

    <%= render 'left_column' %>
    
    <div class="right_column">
        <%= render 'form',tweet: @tweet %>
        
        <%= form_for @tweet, url: {action: :create} do |f| %>
            <%= f.label :message, 'つぶやき' %>
            <%= f.text_field :message %>
            <%= f.submit '投稿' %>
        <% end %>
    </div>
</div>